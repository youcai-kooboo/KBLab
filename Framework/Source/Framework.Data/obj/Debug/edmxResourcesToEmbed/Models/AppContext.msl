    <EntitySetMapping Name="AccessTokens">
      <EntityTypeMapping TypeName="Framework.Data.Models.AccessToken">
        <MappingFragment StoreEntitySet="AccessToken">
          <ScalarProperty Name="Token" ColumnName="Token" />
          <ScalarProperty Name="CreatedOnUtc" ColumnName="CreatedOnUtc" />
          <ScalarProperty Name="ExpirationDateUtc" ColumnName="ExpirationDateUtc" />
